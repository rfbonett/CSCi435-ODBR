        </activity>
        <activity
            android:name=".EndRecordActivity"
            android:label="@string/title_activity_end_record_state" />
        <activity
            android:name=".SubmitReportActivity"
            android:label="@string/title_activity_submit_report"></activity>
    </application>

</manifest>
